            <component
              refId="Package\ETL dim_customer\ODBC Source"
              componentClassID="Attunity.SSISODBCSrc"
              description="ODBC Source"
              name="ODBC Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
    CAST(`ce`.`entity_id` AS SIGNED INT) AS `customer_bk`,  -- business key
    CAST(CONCAT_WS(' ', 
        COALESCE(`firstname`.`value`, ''), 
        COALESCE(`lastname`.`value`, '')
    ) AS VARCHAR(255)) AS `customer_name`,  -- Nombre del cliente

    CAST(COALESCE(`cae`.`street`, 'Sin datos de dirección') AS VARCHAR(255)) AS `customer_address`,  -- Dirección del cliente
    CAST(COALESCE(`cae`.`city`, 'Sin datos de ciudad') AS VARCHAR(255)) AS `customer_city`,  -- Ciudad del cliente
    CAST(COALESCE(`cae`.`region`, 'Sin datos de región') AS VARCHAR(255)) AS `customer_region`,  -- Región del cliente
    CAST(COALESCE(`ce`.`email`, 'Sin datos de email') AS VARCHAR(100)) AS `customer_email`,  -- Email del cliente

    -- Obtener segmento del cliente (grupo al que pertenece)
    CAST(IFNULL(`cg`.`customer_group_code`, 'Sin Grupo') AS VARCHAR(100)) AS `customer_segment`, -- Segmento del cliente (grupo)

    -- Lógica para determinar el loyalty_status solo si pertenece a un grupo
    CAST(CASE 
        WHEN `cg`.`customer_group_code` IS NOT NULL THEN 
            'Leal'  -- Aquí puedes poner la lógica que desees para clientes en grupos
        ELSE 
            'No Leal'  -- Valor para aquellos que no pertenecen a ningún grupo
    END AS VARCHAR(100)) AS `loyalty_status`  -- Columna de loyalty_status

FROM 
    `customer_entity` AS `ce`  -- Tabla principal de clientes

-- Unir con las direcciones de los clientes
LEFT JOIN `customer_address_entity` AS `cae` 
    ON `cae`.`parent_id` = `ce`.`entity_id`

-- Unir con los grupos de clientes (segmentos)
LEFT JOIN `customer_group` AS `cg` 
    ON `ce`.`group_id` = `cg`.`customer_group_id`

-- Unir para obtener el firstname
LEFT JOIN `customer_entity_varchar` AS `firstname` 
    ON `firstname`.`entity_id` = `ce`.`entity_id` 
    AND `firstname`.`attribute_id` = 5  -- Se encontró con subconsulta

-- Unir para obtener el lastname
LEFT JOIN `customer_entity_varchar` AS `lastname` 
    ON `lastname`.`entity_id` = `ce`.`entity_id` 
    AND `lastname`.`attribute_id` = 7;</property>
